		front = -1;
		rear = -1;
		QUEUE_SIZE = size;
		itemArr = new char[QUEUE_SIZE];
	}
	
	@Override
	public void enQueue(char ch) {
		
	}

	@Override
	public char deAueue() {
		return 0;
	}

