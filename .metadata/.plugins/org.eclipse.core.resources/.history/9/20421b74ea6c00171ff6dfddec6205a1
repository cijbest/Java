package recursive;

public class RecursiveTest {

	public static void main(String[] args) {

		int n = factorial_iter(5);
		System.out.println(n);
		
		n = factorial(5);
		System.out.println(n);
		
	}
	
	public static int factorial_iter(int n){
		int i;
		int ret = 1;
		for(i = n; i >= 1; i--){
			ret = ret * 1;
		}
		
		return ret;
	}
	
	public static int factorial(int n){
		System.out.println(2);
	}
	
	

}
